#: main.c:264
msgid "Options:\n"
msgstr "Seçenekler:\n"

#: main.c:265
msgid "  -b, -m                      Ignored for compatibility.\n"
msgstr "  -b, -m                      Uyumluluk için yoksayıldı.\n"

#: main.c:267
msgid "  -B, --always-make           Unconditionally make all targets.\n"
msgstr "  -B, --always-make           Tüm hedefler koşulsuz olarak oluşturulur.\n"

#: main.c:269
msgid ""
"  -C DIRECTORY, --directory=DIRECTORY\n"
"                              Change to DIRECTORY before doing anything.\n"
msgstr "  -C DİZİN, --directory=DIZIN Birşey yapmadan önce DİZİNe geçilir.\n"

#: main.c:272
msgid "  -d                          Print lots of debugging information.\n"
msgstr "  -b, -m                      Bir sürü hata ayıklama bilgisi basar.\n"

#: main.c:274
msgid "  --debug[=FLAGS]             Print various types of debugging information.\n"
msgstr "  --debug[=SEÇENEKLER]        Çeşitli türde hata ayıklama bilgileri basar.\n"

#: main.c:276
msgid ""
"  -e, --environment-overrides\n"
"                              Environment variables override makefiles.\n"
msgstr "  -e, --environment-overrides Ortam değişkenleri makefile'ları değiştirir.\n"

#: main.c:279
msgid ""
"  -f FILE, --file=FILE, --makefile=FILE\n"
"                              Read FILE as a makefile.\n"
msgstr ""
"  -f DOSYA, --file=DOSYA, --makefile=DOSYA\n"
"                              DOSYAyı bir makefile olarak okur.\n"

#: main.c:282
msgid "  -h, --help                  Print this message and exit.\n"
msgstr "  -h, --help                  Bu iletiyi basar ve çıkar.\n"

#: main.c:284
msgid "  -i, --ignore-errors         Ignore errors from commands.\n"
msgstr "  -i, --ignore-errors         Komutların ürettiği hataları yoksayar.\n"

#: main.c:286
msgid ""
"  -I DIRECTORY, --include-dir=DIRECTORY\n"
"                              Search DIRECTORY for included makefiles.\n"
msgstr ""
"  -I DİZİN, --include-dir=DİZİN\n"
"                              Eklenecek makefile'ları DİZİNde arar.\n"

#: main.c:289
msgid "  -j [N], --jobs[=N]          Allow N jobs at once; infinite jobs with no arg.\n"
msgstr ""
"  -j [N], --jobs[=N]          Bir defada N işe izin verir; argumansız iş\n"
"                              sayısı sınırsızdır.\n"

#: main.c:291
msgid "  -k, --keep-going            Keep going when some targets can't be made.\n"
msgstr "  -k, --keep-going            Bazı hedefler yapılmadığında devam eder.\n"

#: main.c:293
msgid ""
"  -l [N], --load-average[=N], --max-load[=N]\n"
"                              Don't start multiple jobs unless load is below N.\n"
msgstr ""
"  -l [N], --load-average[=N], --max-load[=N]\n"
"                              Yük N'den az olmadıkça çoklu işler başlatılmaz.\n"

#: main.c:296
msgid ""
"  -n, --just-print, --dry-run, --recon\n"
"                              Don't actually run any commands; just print them.\n"
msgstr ""
"  -n, --just-print, --dry-run, --recon\n"
"                              Gerçekte komutlar çalıştırılmaz, gösterilir.\n"

#: main.c:299
msgid ""
"  -o FILE, --old-file=FILE, --assume-old=FILE\n"
"                              Consider FILE to be very old and don't remake it.\n"
msgstr ""
"  -o DOSYA, --old-file=DOSYA, --assume-old=DOSYA\n"
"                              DOSYAnın çok eski olduğu varsayılır ve\n"
"                              yeniden işlem yapılmaz.\n"

#: main.c:302
msgid "  -p, --print-data-base       Print make's internal database.\n"
msgstr "  -p, --print-data-base       make'in içsel veritabanını basar.\n"

#: main.c:304
msgid "  -q, --question              Run no commands; exit status says if up to date.\n"
msgstr "  -q, --question              Komut çalıştırmaz; güncelse çıkışta belirtir\n"

#: main.c:306
msgid "  -r, --no-builtin-rules      Disable the built-in implicit rules.\n"
msgstr "  -r, --no-builtin-rules      Oluşumiçi örtük kuralları etkisizleştirir.\n"

#: main.c:308
msgid "  -R, --no-builtin-variables  Disable the built-in variable settings.\n"
msgstr "  -R, --no-builtin-variables  Oluşumiçi değişken ayarlarını etkisizleştirir\n"

#: main.c:310
msgid "  -s, --silent, --quiet       Don't echo commands.\n"
msgstr "  -s, --silent, --quiet     Komutlar işlenirken gösterilmez.\n"

#: main.c:312
msgid ""
"  -S, --no-keep-going, --stop\n"
"                              Turns off -k.\n"
msgstr "  -S, --no-keep-going, --stop Bazı hedefler yapılmadığında devam etmez.\n"

#: main.c:315
msgid "  -t, --touch                 Touch targets instead of remaking them.\n"
msgstr "  -t, --touch                 Yeniden derlemek yerine hedeflere bakıp geçer.\n"

#: main.c:317
msgid "  -v, --version               Print the version number of make and exit.\n"
msgstr "  -v, --version               make sürüm numarasını basar ve çıkar.\n"

#: main.c:319
msgid "  -w, --print-directory       Print the current directory.\n"
msgstr "  -w, --print-directory       Kullanılan dizini basar.\n"

#: main.c:321
msgid "  --no-print-directory        Turn off -w, even if it was turned on implicitly.\n"
msgstr "  --no-print-directory        Dolaylı olarak açılmış olsa bile -w 'yi kapatır\n"

#: main.c:323
msgid ""
"  -W FILE, --what-if=FILE, --new-file=FILE, --assume-new=FILE\n"
"                              Consider FILE to be infinitely new.\n"
msgstr ""
"  -W DOSYA, --what-if=DOSYA, --new-file=DOSYA, --assume-new=DOSYA\n"
"                              DOSYA sonsuz yeni varsayılır.\n"

#: main.c:326
